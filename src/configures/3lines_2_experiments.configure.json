{
    "_comment": "General",
    "random_seed": 1234,


    "_comment": "Data",
    "3lines_2": {
        "path": "../../data/3lines_2",

        "train": {
            "file": "3lines_2_train.txt",
            "num_load": null
        },

        "test": {
            "file": "3lines_2_test.txt",
            "num_load": null
        }
    },


    "_comment": "Model",
    "beta": {
    },

    "linear": {
    },

    "svr": {
        "kernel": "rbf",
        "gamma": 1,
        "C": 10,
        "epsilon": 0,
        "shrinking": false,
        "tol": 0.001
    },

    "decision_tree": {
    },

    "random_forest": {
    },

    "gaussian_process": {
    },


    "_comment": "Training",
    "train":{
        "beta": {
        },

        "svr": {
            "parameter_tuning": false,
            "valid_split": 0.7,
            "C": [0.01, 0.05, 0.1, 0.5, 1, 5, 10, 20, 50, 100],
            "Gamma": [0.01, 0.05, 0.1, 0.5, 1, 5, 10, 20, 50, 100]
        }
    },

    "_comment": "Testing",
    "beta": {
    },


    "_comment": "Log",
    "verbose": false,
    "log_dir": "../outputs/3lines_2/logs/svr",
    "log_file": "run_0107_xx",

    "log": {
        "version": 1,
        "disable_existing_loggers": false,

        "formatters": {
            "custom": {
                "format": "%(asctime)s - [%(name)s - %(funcName)-8.8s:%(lineno)-4d] - [%(levelname)-5.5s] - %(message)s",
                "datefmt": "%Y/%m/%d %H:%M"
            }
        },

        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "DEBUG",
                "formatter": "custom",
                "stream": "ext://sys.stdout"
            },

            "file": {
                "class": "logging.handlers.RotatingFileHandler",
                "level": "DEBUG",
                "formatter": "custom",
                "filename": "../outputs/3lines_2/logs/svr/run_0107_xx",
                "maxBytes": 10485760,
                "backupCount": 20,
                "encoding": "utf8"
            }
        },

        "loggers": {
            "main": {
                "level": "DEBUG",
                "handlers": ["console", "file"],
                "propagate": false
            }
        },

        "root": {
            "level": "DEBUG",
            "handlers": []
        }
    }
}

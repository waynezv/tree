{
    "_comment": "General",
    "random_seed": 1234,


    "_comment": "Data",
    "3lines_2": {
        "path": "../../data/3lines_2",

        "train": {
            "file": "3lines_2_train.txt",
            "num_load": null
        },

        "test": {
            "file": "3lines_2_test.txt",
            "num_load": null
        }
    },


    "_comment": "Model",
    "beta": {
        "random_seed": 1234,

        "_comment": "tree structure",
        "feature_dim": 1,
        "t_low": -6.5,
        "t_high": 7.5,
        "t_step": 1,
        "max_depth": 5,
        "min_samples_split": 20,
        "min_samples_leaf": 10,
        "min_Q_increase": 0.1,

        "_comment": "node expert",
        "svm": {
            "kernel": "rbf",
            "gamma": 1,
            "C": 10,
            "shrinking": false,
            "probability": true,
            "random_state": 1234,
            "tol": 0.001
        },

        "preset_model_params": {
            "probability": true,
            "random_state": 1234,
            "tol": 0.001
        },

        "tuning_settings": {
            "param_grid": {
                "kernel": ["linear", "rbf"],
                "gamma": [0.01, 0.1, 1, 10],
                "C": [0.01, 0.1, 1, 10],
                "shrinking": [true]
            },

            "cv": 3,
            "scoring": ["neg_mean_absolute_error"],
            "iid": false,
            "n_jobs": 32,
            "verbose": 0,
            "refit": "neg_mean_absolute_error",
            "return_train_score": false
        },

        "_comment": "leaf expert",
        "linear":{
        },

        "svr": {
            "default": {
                "kernel": "rbf",
                "gamma": 1,
                "C": 10,
                "epsilon": 0,
                "shrinking": false,
                "tol": 0.001
            },

            "preset_model_params": {
                "tol": 0.001
            },

            "tuning_settings": {
                "param_grid": {
                    "kernel": ["linear", "rbf"],
                    "gamma": [0.01, 0.1, 1, 10],
                    "C": [0.01, 0.1, 1, 10],
                    "epsilon": [0, 0.1],
                    "shrinking": [true, false]
                },

                "cv": 3,
                "scoring": ["neg_mean_absolute_error"],
                "iid": false,
                "n_jobs": 32,
                "verbose": 0,
                "refit": "neg_mean_absolute_error",
                "return_train_score": false
            }
        }
    },

    "linear": {
        "fit_intercept": true,
        "normalize": false
    },

    "svr": {
        "kernel": "rbf",
        "gamma": 1,
        "C": 10,
        "epsilon": 0,
        "shrinking": false,
        "tol": 0.001
    },

    "decision_tree": {
        "criterion": "mae",
        "splitter": "best",
        "max_depth": 5,
        "max_leaf_nodes": null,
        "max_features": null,
        "min_samples_split": 2,
        "min_samples_leaf": 4,
        "min_impurity_decrease": 0,
        "random_state": 1234,
        "presort": false
    },

    "random_forest": {
        "n_estimators": 20,
        "criterion": "mae",
        "max_depth": 5,
        "max_leaf_nodes": null,
        "max_features": null,
        "min_samples_split": 4,
        "min_samples_leaf": 1,
        "min_impurity_decrease": 0,
        "bootstrap": true,
        "n_jobs": 1,
        "random_state": 1234,
        "verbose": 0
    },

    "gaussian_process": {
    },


    "_comment": "Training",
    "train":{
        "beta": {
        },


        "svr": {
            "parameter_tuning": false,
            "valid_split": 0.7,
            "C": [0.01, 0.05, 0.1, 0.5, 1, 5, 10, 20, 50, 100],
            "Gamma": [0.01, 0.05, 0.1, 0.5, 1, 5, 10, 20, 50, 100]
        },


        "decision_tree": {
            "parameter_tuning": false,

            "preset_model_params": {
                "splitter": "best",
                "random_state": 1234,
                "presort": false
            },

            "tuning_settings": {
                "param_grid": {
                    "criterion": ["mse", "mae"],
                    "max_depth": [2, 3, 4, 5, 6, 7, null],
                    "max_leaf_nodes": [null],
                    "max_features": [null],
                    "min_samples_split": [2, 3, 4, 5, 6, 7],
                    "min_samples_leaf": [1, 2, 3, 4, 5, 6],
                    "min_impurity_decrease": [0, 0.1, 0.2, 0.3]
                },

                "cv": 3,
                "scoring": ["neg_mean_absolute_error", "neg_mean_squared_error"],
                "iid": false,
                "n_jobs": 2,
                "verbose": 1,
                "refit": "neg_mean_absolute_error",
                "return_train_score": false
            }
        },


        "random_forest": {
            "parameter_tuning": false,

            "preset_model_params": {
                "random_state": 1234,
                "n_jobs": 1,
                "verbose": 0
            },

            "tuning_settings": {
                "param_grid": {
                    "n_estimators": [1, 5, 10, 15, 20, 25, 30, 35, 40, 45, 50],
                    "criterion": ["mse", "mae"],
                    "max_depth": [2, 3, 4, 5, 6, 7, null],
                    "max_leaf_nodes": [null],
                    "max_features": [null],
                    "min_samples_split": [2, 3, 4, 5, 6, 7],
                    "min_samples_leaf": [1, 2, 3, 4, 5, 6],
                    "min_impurity_decrease": [0, 0.1, 0.2, 0.3],
                    "bootstrap": [true, false]
                },

                "cv": 3,
                "scoring": ["neg_mean_absolute_error", "neg_mean_squared_error"],
                "iid": false,
                "n_jobs": 32,
                "verbose": 1,
                "refit": "neg_mean_absolute_error",
                "return_train_score": false
            }
        }
    },


    "_comment": "Testing",
    "test": {
        "beta": {
        }
    },


    "_comment": "Log",
    "verbose": false,
    "log_dir": "../outputs/3lines_2/logs/tree",
    "log_file": "run_0112_",

    "log": {
        "version": 1,
        "disable_existing_loggers": false,

        "formatters": {
            "custom": {
                "format": "%(asctime)s - [%(name)s - %(funcName)-8.8s:%(lineno)-4d] - [%(levelname)-5.5s] - %(message)s",
                "datefmt": "%Y/%m/%d %H:%M"
            }
        },

        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "DEBUG",
                "formatter": "custom",
                "stream": "ext://sys.stdout"
            },

            "file": {
                "class": "logging.handlers.RotatingFileHandler",
                "level": "DEBUG",
                "formatter": "custom",
                "filename": "../outputs/3lines_2/logs/tree/run_0112_",
                "maxBytes": 10485760,
                "backupCount": 20,
                "encoding": "utf8"
            }
        },

        "loggers": {
            "main": {
                "level": "DEBUG",
                "handlers": ["console", "file"],
                "propagate": false
            }
        },

        "root": {
            "level": "DEBUG",
            "handlers": []
        }
    }
}

{
    "_comment": "General",
    "random_seed": 1234,

    "_comment": "Data",
    "3lines_2": {
        "path": "../data/FEMH_Data/processed/dctcepstra40",

        "train": {
        "num_load": null
        },

        "test": {
        "num_load": null
        }
    },

    "_comment": "Model",
    "svr": {
        "input_dim": 49960,
        "num_classes": 4
    },
    "linear": {
        "input_dim": 16237
    },

    "_comment": "Training",
    "batch_size": 100,
    "num_epochs": 100,

    "_comment": "Testing",
    "valid_freq": 1,
    "num_folds": 5,
    "num_runs": 1,

    "_comment": "Log",
    "verbose": false,
    "log_dir": "../outputs/3lines_2/logs/svr",
    "log_file": "run_",

    "log": {
        "version": 1,
        "disable_existing_loggers": false,

        "formatters": {
            "custom": {
                "format": "%(asctime)s - [%(name)s - %(funcName)-8.8s:%(lineno)-4d] - [%(levelname)-5.5s] - %(message)s",
                "datefmt": "%Y/%m/%d %H:%M"
            }
        },

        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "DEBUG",
                "formatter": "custom",
                "stream": "ext://sys.stdout"
            },

            "file": {
                "class": "logging.handlers.RotatingFileHandler",
                "level": "DEBUG",
                "formatter": "custom",
                "filename": "../outputs/3lines_2/logs/svr/run_",
                "maxBytes": 10485760,
                "backupCount": 20,
                "encoding": "utf8"
            }
        },

        "loggers": {
            "main": {
                "level": "DEBUG",
                "handlers": ["console", "file"],
                "propagate": false
            }
        },

        "root": {
            "level": "DEBUG",
            "handlers": []
        }
    },
}
